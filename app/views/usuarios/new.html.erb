<!-- app/views/usuarios/new.html.erb -->
<%= form_with model: @usuario, local: true do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>
  <div>
    <%= form.label :ciudad %>
    <%= form.text_field :ciudad %>
  </div>
  <div>
    <%= form.label :tlf %>
    <%= form.number_field :tlf %>
  </div>
  <div>
    <%= form.submit "Crear Usuario" %>
  </div>
<% end %>
